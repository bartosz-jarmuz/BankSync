<Window x:Class="BankSync.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BankSync.Windows"
        

        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800">
    <DockPanel LastChildFill="True">

        <Button DockPanel.Dock="Top" x:Name="StartButton" Click="StartButton_OnClick" Content="Start" Margin="10" Height="25"/>
        <WindowsFormsHost Name="FormsHost">
        </WindowsFormsHost>
        <ListBox DockPanel.Dock="Bottom" x:Name="OutputBox"/>

    </DockPanel>
</Window>
